{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nestjs-tutorial/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nestjs-tutorial/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nestjs-tutorial",
  "projects": {
    "...": {
      "type": "application",
      "root": "apps/...",
      "entryFile": "main",
      "sourceRoot": "apps/.../src",
      "compilerOptions": {
        "tsConfigPath": "apps/.../tsconfig.app.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth-lib": {
      "type": "library",
      "root": "libs/auth-lib",
      "entryFile": "index",
      "sourceRoot": "libs/auth-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth-lib/tsconfig.lib.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "common-lib": {
      "type": "library",
      "root": "libs/common-lib",
      "entryFile": "index",
      "sourceRoot": "libs/common-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common-lib/tsconfig.lib.json"
      }
    },
    "database-lib": {
      "type": "library",
      "root": "libs/database-lib",
      "entryFile": "index",
      "sourceRoot": "libs/database-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database-lib/tsconfig.lib.json"
      }
    },
    "email-lib": {
      "type": "library",
      "root": "libs/email-lib",
      "entryFile": "index",
      "sourceRoot": "libs/email-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/email-lib/tsconfig.lib.json"
      }
    },
    "logger-lib": {
      "type": "library",
      "root": "libs/logger-lib",
      "entryFile": "index",
      "sourceRoot": "libs/logger-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/logger-lib/tsconfig.lib.json"
      }
    },
    "nestjs-tutorial": {
      "type": "application",
      "root": "apps/nestjs-tutorial",
      "entryFile": "main",
      "sourceRoot": "apps/nestjs-tutorial/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nestjs-tutorial/tsconfig.app.json"
      }
    },
    "product-service": {
      "type": "application",
      "root": "apps/product-service",
      "entryFile": "main",
      "sourceRoot": "apps/product-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product-service/tsconfig.app.json"
      }
    },
    "redis-lib": {
      "type": "library",
      "root": "libs/redis-lib",
      "entryFile": "index",
      "sourceRoot": "libs/redis-lib/src",
      "compilerOptions": {
        "tsConfigPath": "libs/redis-lib/tsconfig.lib.json"
      }
    },
    "tenant-service": {
      "type": "application",
      "root": "apps/tenant-service",
      "entryFile": "main",
      "sourceRoot": "apps/tenant-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/tenant-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    }
  }
}